{
  "name": "asmcdae-mobile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:dev": "react-native run-android --variant=debug",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "debug": "react-native start --reset-cache && react-native run-android",
    "devtools": "react-devtools",
    "lint": "eslint .",
    "test": "jest",
    "build:release-apk": "cd android && ./gradlew assembleRelease",
    "build:release-aab": "cd android && ./gradlew bundleRelease",
    "build:release": "npm run build:release-apk && npm run build:release-aab",
    "clean": "cd android && ./gradlew clean",
    "clean:all": "npm run clean && rm -rf android/app/build && rm -rf android/build"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/datetimepicker": "^8.4.2",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/drawer": "^7.5.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.10.0",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-calendars": "^1.1313.0",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-image-picker": "^7.1.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "^3.6.1",
    "react-native-redash": "^18.1.3",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.11.1",
    "react-native-toast-message": "^2.3.3",
    "react-native-vector-icons": "^10.2.0",
    "react-native-webview": "^13.15.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "^18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native/babel-preset": "0.79.5",
    "@react-native/eslint-config": "0.79.5",
    "@react-native/metro-config": "0.79.5",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.56.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-devtools": "^6.1.5",
    "react-native-debugger": "^1.1.0",
    "react-test-renderer": "19.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}